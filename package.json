    "@mdx-js/react": "^2.3.0",
    "@orama/plugin-nextra": "^1.2.3",
    "@szhsin/react-menu": "4.1.0",
    "@types/lodash": "4.14.199",
    "chroma-js": "2.4.2",
    "classnames": "2.3.2",
    "js-yaml": "4.1.0",
    "js-yaml-loader": "1.2.2",
    "lodash": "4.17.21",
