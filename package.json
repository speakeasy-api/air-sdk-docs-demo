    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "format": "npm run lint -- --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
