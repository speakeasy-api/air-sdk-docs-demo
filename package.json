{
  "name": "<no value>",
  "version": "0.2.0",
  "description": "Nextra docs template",
  "private": true,
  "scripts": {
    "watch": "while sleep 5; do make; done",
    "dev": "next dev",
    "dev-watch": "next dev & npm run watch &>/dev/null",
    "prebuild": "make",
    "next:build": "next build",
    "export": "next export",
